{
  "name": "credit-app",
  "version": "1.0.0",
  "description": "credit-app",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "tsnd --project tsconfig.json --files --respawn --transpile-only --ignore-watch node_modules -r tsconfig-paths/register -- ./src/app/index.ts",
    "build": "npm run build:clean && npm run lint && npm run test && npm run build:tsc",
    "build:clean": "rm -f tsconfig.build.tsbuildinfo && rm -Rf ./dist/* ; exit 0",
    "build:tsc": "tsc -p tsconfig.build.json && resolve-tspaths -p tsconfig.json -s ./src -o ./dist",
    "start": "node ./dist/index.js",
    "test": "jest --runInBand --detectOpenHandles",
    "lint": "eslint ./**/*.ts",
    "lint:fix": "eslint ./**/*.ts --fix"
  },
  "keywords": [],
  "author": "Michel Roberto Tra√±a Tablada <mtraatabladaa94@gmail.com> (https://soymichel.dev)",
  "license": "MIT",
  "devDependencies": {
    "@types/faker": "5.1.7",
    "@types/jest": "29.0.3",
    "@types/node": "18.7.18",
    "@types/uuid": "^8.3.4",
    "@types/uuid-validate": "^0.0.1",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "eslint": "8.23.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.0.4",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "jest": "29.0.3",
    "prettier": "2.7.1",
    "resolve-tspaths": "0.7.5",
    "ts-jest": "29.0.1",
    "ts-node": "10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "tslib": "2.4.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.8.3"
  },
  "dependencies": {
    "@sentry/node": "7.13.0",
    "@sentry/tracing": "7.13.0",
    "@types/express": "4.17.14",
    "@types/glob": "7.1.3",
    "@types/helmet": "4.0.0",
    "@types/mongodb": "^4.0.7",
    "@types/swagger-ui-express": "^4.1.3",
    "body-parser": "1.20.0",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-promise-router": "^4.1.1",
    "faker": "5.4.0",
    "glob": "7.1.6",
    "helmet": "^6.0.0",
    "http-status": "^1.5.3",
    "mongodb": "^4.9.1",
    "node-dependency-injection": "2.6.11",
    "swagger-ui-express": "^4.5.0",
    "uuid": "^9.0.0",
    "uuid-validate": "^0.0.3"
  }
}
